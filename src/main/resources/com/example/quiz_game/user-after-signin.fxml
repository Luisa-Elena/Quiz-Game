   <children>
      <Label layoutX="14.0" layoutY="14.0" text="You are connected as a user">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Label layoutX="14.0" layoutY="41.0" text="Click on the button below to see the available quiz categories:">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button fx:id="getCategoriesButton" layoutX="14.0" layoutY="69.0" onAction="#onGetCategoriesClick" text="Get Categories" />
   </children>
</VBox>
